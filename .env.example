# 酒店停车管理系统 - 环境变量配置

# 数据库连接 URL（本地开发使用）
# SQLite (本地开发): file:./dev.db
DATABASE_URL="file:./dev.db"

# JWT 配置
# 生产环境请使用强随机字符串，至少 32 字符
JWT_SECRET="your-super-secret-jwt-key-change-in-production-32chars"

# 超级管理员凭据
SUPER_ADMIN_USERNAME="test"
SUPER_ADMIN_PASSWORD="test@2026"

# 新管理员默认密码
DEFAULT_ADMIN_PASSWORD="changeme@123456"

# 默认使用次数
DEFAULT_USE_COUNT="3"

# 应用 URL (用于 CORS 和重定向)
NEXT_PUBLIC_APP_URL="http://localhost:3000"
